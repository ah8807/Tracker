<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">
    <Button
            android:text="Button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/btnAddH"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/btnDecH"
            app:layout_constraintTop_toBottomOf="@+id/textView" android:layout_marginTop="10dp"/>
    <Button
            android:text="Button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/btnDecH"
            app:layout_constraintEnd_toEndOf="parent"
            tools:layout_editor_absoluteY="39dp" android:layout_marginEnd="72dp"/>
    <Button
            android:text="Button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/btnGuitar"
            tools:layout_editor_absoluteX="140dp" app:layout_constraintTop_toBottomOf="@+id/textView2"
            android:layout_marginTop="10dp"/>
    <Button
            android:text="Button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/btnExercise"
            tools:layout_editor_absoluteX="148dp" app:layout_constraintTop_toBottomOf="@+id/textView3"
            android:layout_marginTop="10dp"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="10dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView2"
            tools:layout_editor_absoluteX="109dp" app:layout_constraintTop_toBottomOf="@+id/btnDecH"
            android:layout_marginTop="10dp"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView3"
            tools:layout_editor_absoluteX="154dp" app:layout_constraintTop_toBottomOf="@+id/btnGuitar"
            android:layout_marginTop="10dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>